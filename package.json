{
  "name": "@hanapp-ph/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node dist/apps/api/main",
    "start:dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:web": "nx serve @hanapp-ph/web --turbo",
    "dev:web:webpack": "nx serve @hanapp-ph/web",
    "dev:landing": "nx dev @hanapp-ph/landing",
    "dev:landing:webpack": "nx dev @hanapp-ph/landing",
    "dev:api": "nodemon --watch apps/api/src --ext ts --exec \"ts-node -P apps/api/tsconfig.app.json apps/api/src/main.ts\"",
    "build:web": "nx build @hanapp-ph/web",
    "build:landing": "nx build @hanapp-ph/landing",
    "build:api": "nx build @hanapp-ph/api",
    "build:all": "nx run-many -t build",
    "build:affected": "nx affected -t build",
    "test:web": "nx test @hanapp-ph/web",
    "test:landing": "nx test @hanapp-ph/landing",
    "test:api": "nx test @hanapp-ph/api",
    "test:all": "nx run-many -t test",
    "test:affected": "nx affected -t test",
    "test:watch": "nx run-many -t test --watch",
    "test:coverage": "nx run-many -t test --coverage",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "lint:affected": "nx affected -t lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "e2e:web": "node scripts/run-web-e2e-tests.js",
    "e2e:web:cypress": "nx e2e @hanapp-ph/web-e2e",
    "e2e:affected": "nx affected -t e2e",
    "typecheck": "nx run-many -t typecheck",
    "typecheck:affected": "nx affected -t typecheck",
    "graph": "nx graph",
    "clean": "nx reset && rm -rf dist node_modules",
    "clean:cache": "nx reset",
    "dep-graph": "nx dep-graph",
    "affected:graph": "nx affected:graph",
    "prepare": "husky",
    "precommit": "lint-staged",
    "prepush": "npm run test:affected && npm run lint:affected",
    "release": "standard-version",
    "docker:build": "docker build -t hanapp-ph .",
    "docker:build:api": "docker build --target api-runner -t hanapp-ph-api .",
    "docker:build:web": "docker build --target web-runner -t hanapp-ph-web .",
    "docker:run": "docker run -p 3001:3001 hanapp-ph-api",
    "docker:run:api": "docker run -p 3001:3001 hanapp-ph-api",
    "docker:run:web": "docker run -p 3000:3000 hanapp-ph-web",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod": "docker-compose up -d",
    "docker:prod:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker system prune -f"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/cypress": "21.5.1",
    "@nx/eslint": "21.5.1",
    "@nx/eslint-plugin": "21.5.1",
    "@nx/jest": "21.5.1",
    "@nx/js": "21.5.1",
    "@nx/nest": "^21.5.1",
    "@nx/next": "^21.5.1",
    "@nx/node": "21.5.1",
    "@nx/react": "^21.5.1",
    "@nx/web": "21.5.1",
    "@nx/webpack": "21.5.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.38",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "autoprefixer": "10.4.13",
    "babel-jest": "^30.0.2",
    "concurrently": "^9.2.1",
    "cypress": "^14.5.4",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.10",
    "nx": "21.5.1",
    "postcss": "8.4.38",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.544.0",
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,scss}": [
      "prettier --write"
    ]
  }
}
