{
  "name": "@hanapp-ph/landing",
  "sourceRoot": "apps/landing",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "landing:build:development",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "landing:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "landing:build:production",
          "dev": false
        }
      }
    },
    "start": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "landing:build:production",
        "dev": false,
        "port": 3001
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "landing:build:production"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/landing/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/landing/jest.config.cjs",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "coverageReporters": ["text"]
        }
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --skipLibCheck",
        "cwd": "apps/landing"
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/landing"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/landing"
        },
        "production": {
          "outputPath": "dist/apps/landing"
        }
      }
    }
  },
  "tags": ["type:app", "scope:landing"]
}
