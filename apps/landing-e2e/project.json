{
  "name": "@hanapp-ph/landing-e2e",
  "sourceRoot": "apps/landing-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/landing-e2e/cypress.config.ts",
        "devServerTarget": "@hanapp-ph/landing:serve:development",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "@hanapp-ph/landing:serve:production"
        },
        "ci": {
          "baseUrl": "http://localhost:3001"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/landing-e2e/**/*.{js,ts}"]
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project apps/landing-e2e/tsconfig.json"
      }
    }
  },
  "tags": ["type:e2e", "scope:landing"],
  "implicitDependencies": ["@hanapp-ph/landing"]
}
