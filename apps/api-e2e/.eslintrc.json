{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "env": {
    "jest": true,
    "node": true
  },
  "overrides": [
    {
      "files": ["*.ts", "*.js"],
      "parser": "@typescript-eslint/parser",
      "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
      },
      "rules": {
        "no-undef": "off"
      }
    }
  ],
  "rules": {}
}
